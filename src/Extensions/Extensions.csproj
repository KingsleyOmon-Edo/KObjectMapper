<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>A library of useful C# extension methods</Title>
		<PackageId>ExtensionsLibraryMix</PackageId>
		<Version>0.0.1-0.0.1.$([System.Guid]::NewGuid().ToString())</Version>
		<Author>Kingsley Omnon-Edo</Author>
		<Company>Test Company</Company>
		<PackageDescription>A collection of useful extensioin methods</PackageDescription>
		<RepositoryUrl>https://github.com/KingsleyOmon-Edo/Libraries.git</RepositoryUrl>		
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json.Bson" Version="1.0.2" />
	</ItemGroup>

</Project>
